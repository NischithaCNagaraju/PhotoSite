<!Doctype html>
<html lang="en">
<body>
<div>
 <h1>Photosite - ProjectR1</h1>
  <h2>List of Users</h2>
</div>
<table>
  <tr>
  <th>User Id</th>
  <th>First Name</th>
  <th>Last Name</th>
  </tr>

  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= link_to user.id, user_photos_path(user.id) %></td>
    <td><%= link_to user.first_name, user_photos_path(user.id) %></td>
    <td><%= link_to user.last_name, user_photos_path(user.id) %></td>



  </tr>
 <%end%>

  </tbody>

</table>

</body>


</html>



